<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.miretsky.ron.myapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.project.platform.Game.MainActivity" />
        <activity android:name="com.project.platform.Game.RegisterActivity" />
        <activity android:name="com.project.platform.Game.LogInActivity" />
        <activity android:name="com.project.platform.Game.ForgotPasswordActivity" />
        <activity android:name="com.project.platform.Game.NewPassActivity" />
        <activity android:name="com.project.platform.Game.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.project.platform.Game.ConfirmCodeActivity"></activity>
    </application>

</manifest>